/***************************** Include Files *******************************/

#include "altavoz.h"

/************************** Function Definitions ***************************/

void my_delay ( int delay ){
	int i, j;
	for (i =0; i<delay; i=i+1)
		for (j =0; j <500; j=j +1){}
}

void ALTAVOZ_calla(){
	ALTAVOZ mWriteSlaveReg0 ( XPAR_ALTAVOZ_0_BASEADDR , 0, SILENCIO );
}
